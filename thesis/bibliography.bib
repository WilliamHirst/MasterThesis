  keywords = {High Energy Physics - Experiment, Physics - Instrumentation and Detectors},
  file = {arXiv Fulltext PDF:/Users/williamhirst/Zotero/storage/7AWWJNLE/ATLAS Collaboration - 2020 - Performance of the ATLAS muon triggers in Run 2.pdf:application/pdf;arXiv.org Snapshot:/Users/williamhirst/Zotero/storage/YRTRTRAA/2004.html:text/html},
}

@article{atlas_collaboration_photons,
